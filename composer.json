{
    "name": "xisio/yii2-app-api",
    "description": "Yii Framework Application Template for quickly building API-first applications.Based on cebe/yii2-app-api",
    "type": "project",
    "license": "MIT",
	"minimum-stability":"dev",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.16",
        "cebe/php-openapi": "^1.1",
        "cebe/yii2-openapi": "dev-master",
        "yiisoft/yii2-gii": "~2.1.0",
        "ramsey/uuid": "^3.0@dev",
        "kartik-v/yii2-export": "@dev",
        "mootensai/yii2-enhanced-gii": "dev-master",
        "xisio/yii2-rbac-generator": "dev-master",
        "yiisoft/yii2-bootstrap4": "^1.0@dev",
        "yiisoft/yii2-bootstrap": "^2.0@dev",
        "yii2mod/yii2-rbac": "^2.3",
        "yii2tech/admin": "^1.0@dev",
        "2amigos/yii2-usuario": "dev-master",
        "kartik-v/yii2-datecontrol": "^1.9@dev",
        "kartik-v/yii2-bootstrap4-dropdown": "^1.0@dev",
        "kartik-v/yii2-widget-fileinput": "dev-master",
        "mohorev/yii2-upload-behavior": "dev-master",
        "yiisoft/yii2-debug": "^2.0@dev",
        "rmrevin/yii2-fontawesome": "dev-master",
        "kartik-v/yii2-icons": "@dev",
        "symfony/inflector": "4.4.x-dev",
        "yiisoft/yii2-apidoc": "@dev",
        "kartik-v/yii2-editable": "@dev",
	"mootensai/yii2-uuid-behavior": "@dev"
    },
    "autoload": {
        "psr-4": {
            "api\\": "api/",
            "backend\\": "backend/",
            "console\\": "console/",
            "common\\": "common/"
        }
    },
    "config": {
        "platform": {"php": "7.1.3"},
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
          "type": "vcs",
          "url": "https://github.com/xisio/yii2-enhanced-gii"
        },
        {
          "type": "vcs",
          "url": "https://github.com/xisio/yii2-openapi"
        },
	{
          "type": "vcs",
          "url": "https://github.com/xisio/yii2-uuid-behavior"
        }

    ]
}
